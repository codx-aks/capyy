<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logocapyrun"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logocapyrun"
        android:supportsRtl="true"
        android:theme="@style/Theme.Capyy"
        tools:targetApi="31">
        <activity
            android:name=".info"
            android:exported="false" />
        <activity
            android:name=".playerz"
            android:exported="false"
            android:label="@string/title_activity_playerz"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.Capyy.NoActionBar" />
        <activity
            android:name=".spalshactivity"
            android:exported="true"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MainActivity1"
            android:exported="true"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MainActivity2"
            android:exported="true"
            android:screenOrientation="landscape"
            tools:ignore="IntentFilterExportedReceiver">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>